{
  "name": "feeds",
  "version": "1.0.0",
  "engines": {
    "node": "12.x"
  },
  "type": "module",
  "scripts": {
    "test": "jest",
    "lint": "eslint .",
    "format": "prettier --write \"**/*.{md,json,yml,html,js}\""
  },
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/preset-env": "^7.8.7",
    "babel-plugin-transform-default-import": "^1.0.0",
    "eslint": "^6.8.0",
    "jest": "^25.1.0",
    "prettier": "^1.19.1"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ],
    "plugins": [
      "transform-default-import"
    ]
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true,
      "jest": true,
      "es6": true
    },
    "parserOptions": {
      "ecmaVersion": 2020,
      "sourceType": "module"
    },
    "extends": [
      "eslint:recommended"
    ]
  }
}
